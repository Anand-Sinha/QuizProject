<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="favicon.ico">
    <link rel="stylesheet" href="/css/style.css" />
    <title>Quiz App</title>
  </head>
  <body>
    <section class="container">
      <button class="start-btn btn">Start Quiz</button>
      <span class="score hide">Score:- 5</span>
      <span class="count-down"></span>
      <div class="main hide">
        <div class="question__container">
          <span class="question__text">Rate This App!</span>
          <div class="answer__container">
            <button class="answer__btn btn" type="button">1*</button>
            <button class="answer__btn btn" type="button">2*</button>
            <button class="answer__btn btn" type="button">3*</button>
            <button class="answer__btn btn" type="button">4*</button>
            <button class="answer__btn btn" type="button">5*</button>
          </div>
        </div>
        <div class="action__container">
          <button class="next__btn btn" type="button">Next</button>
        </div>
      </div>
    </section>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script type="module" src="/js/index.js"></script>
  </body>
</html>
